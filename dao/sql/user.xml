<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="USER">
  <select id="SelectUserList">
    SELECT
      id,
      name,
      email,
      created_at
    FROM
      user
    WHERE 1=1
  </select>
</mapper>